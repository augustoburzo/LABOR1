<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="order_insert_window" named="True">
    <property name="geometry">640x480</property>
    <property name="height">600</property>
    <property name="resizable">none</property>
    <property name="title" translatable="yes">Inserisci ordine</property>
    <property name="width">800</property>
    <containerlayout manager="grid">
      <property type="col" id="0" name="weight">1</property>
      <property type="row" id="1" name="weight">1</property>
    </containerlayout>
    <child>
      <object class="ttk.Labelframe" id="customer_lf" named="True">
        <property name="height">200</property>
        <property name="text" translatable="yes">Anagrafica cliente</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">0</property>
          <property name="sticky">nsew</property>
        </layout>
        <containerlayout manager="grid">
          <property type="col" id="1" name="weight">1</property>
          <property type="row" id="all" name="pad">5</property>
        </containerlayout>
        <child>
          <object class="ttk.Label" id="customer_cod_label" named="True">
            <property name="text" translatable="yes">Codice:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="customer_cod_entry" named="True">
            <property name="state">readonly</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">5</property>
              <property name="row">0</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="customer_lastname_lbl" named="True">
            <property name="text" translatable="yes">Cognome:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">1</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="customer_lastname_entry" named="True">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">5</property>
              <property name="row">1</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="customer_firstname_lbl" named="True">
            <property name="text" translatable="yes">Nome:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">2</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="customer_firstname_entry" named="True">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">5</property>
              <property name="row">2</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="customer_phone_lbl" named="True">
            <property name="text" translatable="yes">Cellulare:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">3</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="customer_phone_entry" named="True">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">5</property>
              <property name="row">3</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="search_btn" named="True">
            <property name="command" type="command" cbtype="simple">on_search_btn_press</property>
            <property name="text" translatable="yes">Cerca...</property>
            <layout manager="grid">
              <property name="column">2</property>
              <property name="padx">5</property>
              <property name="pady">5</property>
              <property name="row">0</property>
              <property name="rowspan">4</property>
              <property name="sticky">nsew</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Labelframe" id="product_lf" named="True">
        <property name="height">200</property>
        <property name="text" translatable="yes">Anagrafica prodotto</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="pady">0 10</property>
          <property name="row">1</property>
          <property name="sticky">nsew</property>
        </layout>
        <containerlayout manager="grid">
          <property type="col" id="1" name="weight">1</property>
          <property type="col" id="all" name="pad">5</property>
          <property type="row" id="2" name="weight">1</property>
          <property type="row" id="all" name="pad">5</property>
        </containerlayout>
        <child>
          <object class="ttk.Label" id="product_name_lbl" named="True">
            <property name="text" translatable="yes">Prodotto:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="product_name_entry" named="True">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">5</property>
              <property name="row">0</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="serial_number_lbl" named="True">
            <property name="text" translatable="yes">Seriale:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">1</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="serial_number_entry" named="True">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">5</property>
              <property name="row">1</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="malfunction_lbl" named="True">
            <property name="text" translatable="yes">Difetto:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">2</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Text" id="malfunction_txt" named="True">
            <property name="height">10</property>
            <property name="width">50</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">5</property>
              <property name="pady">0 5</property>
              <property name="row">2</property>
              <property name="sticky">nsew</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="insert_order_btn" named="True">
        <property name="command" type="command" cbtype="simple">on_insert_order_btn_press</property>
        <property name="text" translatable="yes">Inserisci ordine</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="pady">0 10</property>
          <property name="row">2</property>
          <property name="sticky">nsew</property>
        </layout>
      </object>
    </child>
  </object>
  <object class="tk.Toplevel" id="existant_customer_window" named="True">
    <property name="geometry">640x480</property>
    <property name="height">200</property>
    <property name="resizable">none</property>
    <property name="takefocus">true</property>
    <property name="title" translatable="yes">Seleziona cliente esistente</property>
    <property name="width">200</property>
    <child>
      <object class="ttk.Labelframe" id="customers_lf" named="True">
        <property name="height">200</property>
        <property name="text" translatable="yes">Clienti esistenti</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="padx">10</property>
          <property name="pady">10 5</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="ttk.Treeview" id="customers_tv" named="True">
            <property name="selectmode">extended</property>
            <property name="show">headings</property>
            <layout manager="pack">
              <property name="expand">true</property>
              <property name="fill">both</property>
              <property name="padx">5</property>
              <property name="pady">5</property>
              <property name="side">top</property>
            </layout>
            <child>
              <object class="ttk.Treeview.Column" id="customer_code_tab" named="True">
                <property name="column_anchor">w</property>
                <property name="heading_anchor">w</property>
                <property name="minwidth">60</property>
                <property name="stretch">true</property>
                <property name="text" translatable="yes">Cod.</property>
                <property name="tree_column">false</property>
                <property name="visible">true</property>
                <property name="width">60</property>
              </object>
            </child>
            <child>
              <object class="ttk.Treeview.Column" id="customer_first_name_col" named="True">
                <property name="column_anchor">w</property>
                <property name="heading_anchor">w</property>
                <property name="minwidth">20</property>
                <property name="stretch">true</property>
                <property name="text" translatable="yes">Nome</property>
                <property name="tree_column">false</property>
                <property name="visible">true</property>
                <property name="width">200</property>
              </object>
            </child>
            <child>
              <object class="ttk.Treeview.Column" id="customer_last_name_col" named="True">
                <property name="column_anchor">w</property>
                <property name="heading_anchor">w</property>
                <property name="minwidth">20</property>
                <property name="stretch">true</property>
                <property name="text" translatable="yes">Cognome</property>
                <property name="tree_column">false</property>
                <property name="visible">true</property>
                <property name="width">200</property>
              </object>
            </child>
            <child>
              <object class="ttk.Treeview.Column" id="customer_address_col" named="True">
                <property name="column_anchor">w</property>
                <property name="heading_anchor">w</property>
                <property name="minwidth">20</property>
                <property name="stretch">true</property>
                <property name="text" translatable="yes">Indirizzo</property>
                <property name="tree_column">false</property>
                <property name="visible">true</property>
                <property name="width">200</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="new_customer_btn" named="True">
        <property name="command" type="command" cbtype="simple">on_new_customer_btn_press</property>
        <property name="text" translatable="yes">Inserisci nuovo cliente...</property>
        <layout manager="pack">
          <property name="fill">x</property>
          <property name="padx">10</property>
          <property name="pady">0 10</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
