<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="credits_window" named="True">
    <property name="geometry">640x480</property>
    <property name="height">200</property>
    <property name="iconbitmap">labor.ico</property>
    <property name="resizable">none</property>
    <property name="title" translatable="yes">Credits</property>
    <property name="width">200</property>
    <containerlayout manager="grid">
      <property type="col" id="0" name="weight">1</property>
      <property type="row" id="1" name="weight">1</property>
    </containerlayout>
    <child>
      <object class="ttk.Label" id="label1">
        <property name="image">LABOR1_small.png</property>
        <property name="justify">center</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="pady">5</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label2">
        <property name="font">{Arial} 20 {bold}</property>
        <property name="foreground">#004080</property>
        <property name="justify">center</property>
        <property name="text" translatable="yes">Autore: Augusto Burzo
Copyright 2023 - Augusto Burzo
Tutti i diritti riservati
Contatti: info@augustoburzo.com</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="pady">10</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label3">
        <property name="font">{Arial} 12 {bold}</property>
        <property name="foreground">#004080</property>
        <property name="justify">center</property>
        <property name="relief">groove</property>
        <property name="text" translatable="yes">Le icone utilizzate sono concesse da:
https://icons8.com</property>
        <bind sequence="&lt;Button-1&gt;" handler="callback" add="" />
        <layout manager="grid">
          <property name="column">0</property>
          <property name="pady">10</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
